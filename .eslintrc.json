{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-unused-vars": [
      "error",
      { "varsIgnorePattern": "^(cookies|removeCookie|setCookie)$" }
    ]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
